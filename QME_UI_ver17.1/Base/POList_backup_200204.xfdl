	var cCid = this.ds_ColChangeHDR.getColumn(this.ds_ColChangeHDR.rowposition, "CID");
	for(var i = 0 ; i < this.ds_ColChange.rowcount ; i++)
	{
		if(this.ds_ColChange.getColumn(i, "CID") == cCid)
		{
			var dRow = this.ds_ColChange.getColumn(i, "DsRow");
			var dCol = this.ds_ColChange.getColumn(i, "DsCol");
			var dVal = this.ds_ColChange.getColumn(i, "DsNewVal");
			
			bDsColChangeHistExec = false;
			this.ds_POList.setColumn(dRow, dCol, dVal);
			bDsColChangeHistExec = true;
		}
	}
};
